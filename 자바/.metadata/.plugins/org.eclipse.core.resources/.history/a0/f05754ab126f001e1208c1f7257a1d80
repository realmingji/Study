import java.util.*;

public class Main {

	public static void main(String[] args) {
		LinkedList<Character> linkedlist = new LinkedList<>();
		
		linkedlist.add('K');
		linkedlist.add('O');
		linkedlist.add('R');
		linkedlist.add('E');
		linkedlist.add('A');
		
		for(int i = 0; i<linkedlist.size(); i++) {
			System.out.println(linkedlist.get(i));
		}
	}

}
