
public class Main {

	public static void main(String[] args) {
		int x = 9, y = -9, z = 5;
		double angle = 60.0;
		
		System.out.println(Math.PI);
		System.out.println(Math.E);
		
		System.out.println(Math.ceil(3.45));	// 올림
		System.out.println(Math.floor(3.56));	// 내림
		
		System.out.println(Math.round(3.34));	// 반올림
		System.out.println(Math.round(3.78));	// 반올림
	}

}
